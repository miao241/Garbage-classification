/* custom-tab-bar/index.wxss */
.tab-bar {
  position: fixed;
  bottom: 0px;
  left: 0px;
  right: 0px;
  width: 100%;
  height: 110rpx;
  background:white;
  display: flex;
  flex-direction: row;
  padding-bottom: env(safe-area-inset-bottom);
  border-top: 1rpx solid #b4a9a9; 
}

