.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-top: 16rpx;
}

.cover-image {
  width: 56rpx;
  height: 56rpx;
}

.tab-bar-item cover-view {
  font-size: 20rpx;
}


.centerImage {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  bottom: 10rpx;
  border-radius: 50%;
